@{
ViewData["Title"] = "Our Menu";
}

@model List<BirdAndBrewMVC.Models.MenuItem>


<div class="container">
    <div class="py-5 text-center">
        <img class="d-block mx-auto mb-4" src="~/menu.svg" alt="" width="372" height="372">
    </div>
</div>

    <div class="d-flex flex-wrap">
    @foreach (var item in Model)
    {
        @await Html.PartialAsync("_MenuItemCard", item)
    }
</div>

