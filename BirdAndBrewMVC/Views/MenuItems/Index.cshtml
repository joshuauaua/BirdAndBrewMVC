@{
ViewData["Title"] = "Our Menu";
}

@model List<BirdAndBrewMVC.Models.MenuItem>


<div class="container">
    <div class="py-5 text-center">
        <img class="d-block mx-auto mb-4" src="~/menu.png" alt="" width="272" height="272">
        <h2>Our Menu</h2>
        <p class="lead">From humble beginnings</p>

    </div>
</div>

    <div class="d-flex flex-wrap">
    @foreach (var item in Model)
    {
        @await Html.PartialAsync("_MenuItemCard", item)
    }
</div>

